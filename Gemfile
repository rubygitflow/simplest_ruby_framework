source 'https://rubygems.org'

# ruby '2.7.4'

# A toolkit of support libraries and Ruby core extensions, 
# github: 'https://github.com/rails/rails/tree/v7.0.3/activesupport'
gem 'activesupport'

# The Database Toolkit for Ruby, 
# github: 'https://github.com/jeremyevans/sequel'
gem 'sequel'

# Interface with the SQLite3 database engine, 
# github: 'https://github.com/sparklemotion/sqlite3-ruby'
gem 'sqlite3'

# An easy-to-use client library for making requests from Ruby.
# github: 'https://github.com/httprb/http'
gem "http"

# JSON5 parser for Ruby https://ru.wikipedia.org/wiki/JSON#JSON5
# https://github.com/taichi-ishitani/rb_json5
gem 'rb_json5'
